.body{
    width:100%;
    color:dimgrey;
    font-size: 13px;
    font-family:"Microsoft YaHei";
}
.nav{
	width:100%;
    height:45px;
    font-size: 16px;
    display:flex;
    justify-content:center;
    align-items:center;
}
.container{
    height: 120px;
}
.container image{
    width:100%;
    height: 120px;
}
.ss{
    width: 95%;
    height:53px;
    padding:2.5%;
    text-align: center;
}
.sever{
    float:left;
    width: 25%; 
}
.sever image{
    width:40%;
    height:35px;
    margin: 0 auto; 
    display: block;
}
.tt{
    width: 90%;
    margin:0 auto;
    height:40px;
    font-size:16px;
    line-height: 40px;
    padding:0 5%;
    background-color: #f2f2f2;
}
.scroll{
    width: 100%; 
}
.sevlist{
    width: 100%;
}
.sevlist .single{
    width: 100%;
    padding: 20px 0;
    display:flex;
    flex-flow: row nowrap;
}
.i{
    width: 30%;
    height:100%;
    float:left;
    display: flex;
    justify-content: center;
    align-items: center;
}
.sevlist  image{
    width:75%;
    height:80px;
}
.sevlist .row{
    width: 70%;
    display:flex;
    align-items: center;
}
.vie .title{
    font-size: 18px;
    margin-bottom: 7px;
}
.vie .time{
    margin-bottom: 11px;
}
.vie .info{
    padding: 2px 6px;
    border: 1px solid #9CC5E8;
    border-radius: 10px;
    margin-right:10px; 
}
.space{
    width: 100%;
    height: 15px;
    background-color: #f2f2f2;
}
#cart-page scroll-view {
 height:400px;
}
.btn{
    width: 100px;
    height: 40px;
    font-size: 16px;
    line-height: 40px;
    margin:0 auto;
    border-radius: 10px;
    background-color:#9CC5E8;
    color:#fff; 
}

 